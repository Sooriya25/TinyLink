{
  "version": 2,
  "builds": [
    { "src": "api/server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "^/api/(.*)$", "dest": "api/server.js" },
    { "src": "^/healthz$", "dest": "api/server.js" },
    { "src": "^/code/(.*)$", "dest": "/public/stats.html" },
    { "src": "^/$", "dest": "/public/index.html" },
    { "src": "^(.*)$", "dest": "api/server.js" }
  ]
}
